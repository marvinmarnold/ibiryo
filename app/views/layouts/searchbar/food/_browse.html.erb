<div class="modal hide fade" id="browse-food">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h3><%= t('searchbar.food.browse.popup.title') %></h3>
  </div>
  <div class="modal-body">
    <%= form_tag(root_path, method: :get) do %>
      <% ItemType.find(1).tag_groups.each do |tag_group| %>
        <h5><%= tag_group.name %></h5>
        <% tag_group.tags.each do |tag| %>
          <%= check_box_tag(:food_tag) %>
          <%= label_tag(:food_tag, tag.name) %>
        <% end %>
      <% end %>
    <% end %>
  </div>
  <div class="modal-footer">
  </div>
</div>

<h1>Listing menu_sections</h1>

<table>
  <tr>
    <th>Menu</th>
    <th>Is active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @menu_sections.each do |menu_section| %>
  <tr>
    <td><%= menu_section.menu %></td>
    <td><%= menu_section.is_active %></td>
    <td><%= link_to 'Show', menu_section %></td>
    <td><%= link_to 'Edit', edit_menu_section_path(menu_section) %></td>
    <td><%= link_to 'Destroy', menu_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Menu section', new_menu_section_path %>
